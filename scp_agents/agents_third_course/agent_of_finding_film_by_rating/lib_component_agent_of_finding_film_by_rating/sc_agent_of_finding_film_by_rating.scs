sc_agent_of_finding_film_by_rating
=> nrel_main_idtf:
	[sc-агент поиска фильмов по оценке] (* <- lang_ru;; *);
	[sc-agent of finding film by rating] (* <- lang_en;; *);
	
<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);
	
=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_finding_film_by_rating_condition => ..sc_agent_of_finding_film_by_rating_result);
	
<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	question_of_finding_film_by_rating
	};


=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_finding_film_by_rating
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_finding_film_by_rating		
		};;
		
	-> sc_agent_of_finding_film_by_rating_scp (* <- active_sc_agent;; *);;
	*);;

..sc_agent_of_finding_film_by_rating_condition
= [*
	question_of_finding_film_by_rating _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> rrel_1:: .._param1;;
	.._question _-> rrel_2:: .._param2;;
*];;
..sc_agent_of_finding_film_by_rating_result
= [*
	question_of_finding_film_by_rating _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> rrel_1:: .._param1;;
	.._question _-> rrel_2:: .._param2;;
*];;
